{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "%(getTitle)"
          ]
        }
      }
    },
    "body": {
      "className": "Center",
      "na": {
        "child": {
          "className": "Column",
          "na": {
            "mainAxisAlignment": "#(MainAxisAlignment.center)",
            "children": [
              {
                "className": "Image.asset",
                "pa": [
                  "assets/image/logo.png"
                ]
              },
              {
                "className": "Padding",
                "na": {
                  "padding": {
                    "className": "EdgeInsets.only",
                    "na": {
                      "bottom": 40
                    }
                  },
                  "child": {
                    "className": "Text",
                    "pa": [
                      "#($_count)"
                    ]
                  }
                }
              }
            ]
          }
        }
      }
    },
    "floatingActionButton": {
      "className": "Row",
      "na": {
        "mainAxisAlignment": "#(MainAxisAlignment.end)",
        "children": [
          {
            "className": "FloatingActionButton",
            "na": {
              "child": {
                "className": "Icon",
                "pa": [
                  "#(Icons.add)"
                ]
              },
              "heroTag": "onTapText",
              "onPressed": "@(onTapText)"
            }
          },
          {
            "className": "FloatingActionButton",
            "na": {
              "child": {
                "className": "Icon",
                "pa": [
                  "#(Icons.open_in_new)"
                ]
              },
              "heroTag": "pushNamed",
              "onPressed": {
                "className": "Navigator.pushNamed",
                "pa": [
                  "@(context)",
                  "fair_page"
                ],
                "na": {
                  "arguments": {
                    "name": "动态界面跳转&传值",
                    "path": "assets/bundle/lib_src_page_logic-page2page_sample_logic_page2page.fair.json",
                    "data": {
                      "_count": 58
                    }
                  }
                }
              }
            }
          }
        ]
      }
    }
  },
  "methodMap": {
    "createState": {
      "className": "FairStateWarpper",
      "pa": [
        "%(_State)"
      ]
    }
  }
}
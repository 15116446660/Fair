{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "%(getTitle)"
          ]
        }
      }
    },
    "body": {
      "className": "Center",
      "na": {
        "child": {
          "className": "Column",
          "na": {
            "mainAxisAlignment": "#(MainAxisAlignment.center)",
            "children": [
              {
                "className": "Image.asset",
                "pa": [
                  "assets/image/logo.png"
                ]
              },
              {
                "className": "Padding",
                "na": {
                  "padding": {
                    "className": "EdgeInsets.only",
                    "na": {
                      "bottom": 40
                    }
                  },
                  "child": {
                    "className": "Text",
                    "pa": [
                      "#(Click Count = $_count)"
                    ]
                  }
                }
              }
            ]
          }
        }
      }
    },
    "floatingActionButton": {
      "className": "Row",
      "na": {
        "mainAxisAlignment": "#(MainAxisAlignment.end)",
        "children": [
          {
            "className": "FloatingActionButton",
            "na": {
              "child": {
                "className": "Icon",
                "pa": [
                  "#(Icons.add)"
                ]
              },
              "onPressed": "@(onTapText)"
            }
          }
        ]
      }
    }
  },
  "methodMap": {
    "createState": "%(_State)"
  }
}